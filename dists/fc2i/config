### RED HAT FEDORA CORE 2

### Distcc compilers
DISTCC_CC="i386-redhat-linux-gcc-3.3.3"
DISTCC_CXX="i386-redhat-linux-g++-3.3.3"

UPDATECMD="apt-get -qq update"
UPGRADECMD="apt-get -qq -f -y dist-upgrade"
INSTALLCMD="apt-get -qq -y -m install %p"
CLEANCMD="apt-get clean"

RPMBUILDOPTS='--define "fedora 2"'

DISTDESC="Fedora Core 2 (i386)"
DISTNAME="fc2"
DISTNICK="fc2i"
DISTTAG="1.fc2"
DISTPATH="fedora/2/en/i386"
DISTARCH="i386"

REPO_APT="yes"
REPO_YUM="yes"
REPO_CREATEREPO="yes"
